										</xs:complexType>
									</xs:element>
								</xs:choice>
								<xs:attribute name="SpecialistReview" type="xs:string" use="optional"/>
								<xs:attribute name="Explicit" type="xs:boolean" use="required"/>
							</xs:complexType>
						</xs:element>
