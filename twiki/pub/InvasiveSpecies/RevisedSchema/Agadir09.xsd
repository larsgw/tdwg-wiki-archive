						<xs:element name="SoilDrainage" type="PlaceholderT" minOccurs="0"/>
						<xs:element name="SlopeRange" type="PlaceholderT" minOccurs="0"/>
						<xs:element name="Aspect" type="PlaceholderT" minOccurs="0"/>
