			<xs:element name="DissolvedOxygen" type="PlaceholderT" minOccurs="0"/>
			<xs:element name="WaterVelocity" type="PlaceholderT" minOccurs="0"/>
			<xs:element name="Turbidity" type="PlaceholderT" minOccurs="0"/>
