					<xs:sequence>
						<xs:element name="DefinedSchema" type="DefinedSchemaType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="DateOfFirstObservation" minOccurs="0">
				<xs:complexType/>
			</xs:element>
