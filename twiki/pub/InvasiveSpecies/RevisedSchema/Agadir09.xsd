					<xs:documentation>@@@ Known Dispersal methods</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NativeRange" type="PlaceholderT" minOccurs="0"/>
			<xs:element name="IntroducedRange" type="PlaceholderT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
